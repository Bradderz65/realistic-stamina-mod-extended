{
	"schemaVersion": 1,
	"id": "rstamina",
	"version": "${version}",
	"name": "Realistic Stamina - Extended",
	"description": "An enhanced version of the Realistic Stamina mod by sparkierkan7. Adds configurable jump stamina costs, mining fatigue cooldown, and other improvements to the original stamina system.",
	"authors": [
		"Bradderz65 (Modified version)",
		"sparkierkan7 (Original author)"
	],
	"contact": {
		"homepage": "https://github.com/Bradderz65/realistic-stamina-mod",
		"sources": "https://github.com/Bradderz65/realistic-stamina-mod",
		"issues": "https://github.com/Bradderz65/realistic-stamina-mod/issues"
	},
	"license": "MIT",
	"icon": "assets/rstamina/copper_clock.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"realisticstamina.rstamina.RStaminaMod"
		],
		"client": [
			"realisticstamina.rstamina.RStaminaClient"
		]
	},
	"mixins": [
		"rstamina.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"completeconfig": ">=2.4.0"
	},
	"suggests": {
		"another-mod": "*"
	}
}